a = int(input("Enter a: "))
if a > 1:
    count = 0
    for i in range(1, a + 1):
        if a % i == 0:
            count += 1
    if count == 2:
        print("Պարզ թիվ է")
    else:
        print("Պարզ թիվ չէ")
else:
    print("Պարզ թիվ չէ")


start =0
end = 50
for a in range(start, end + 1):
    count = 0
    for i in range(1, a + 1):
        if a % i == 0:
            count += 1
    if count == 2:
        print(a)

2.dictionary xndir


students = {
    'Armen':{'ruseren':10,'angleren':7,'hayoc lezu':9},
    'Hayk':{'ruseren':7,'angleren':6,'hayoc lezu':5}
}
for name,subjects in students.items():
    avg=sum(subjects.values())/len(subjects)
    print(name,"=",avg)
    
subjects_avg={}
for name,subjects in students.items():
    for subject,grade in subjects.items():
        if subject not in subjects_avg:
            subjects_avg[subject]=[]
        subjects_avg[subject].append(grade)
for subject,grades in subjects_avg.items():
    avg1=sum(grades)/len(grades)

3.wordle game


def wordle(secretword):
	GREEN = "\033[92m"
	ORANGE = "\033[93m"
	RESET = "\033[0m"
	wordletries = 5
	for attempt in range(wordletries):
		print()
		word = input("Enter word: ")
		if len(word) == 5: 
			print()
			if word == secretword:
				print("You won")
				break
			else:
				for i, letter in enumerate(word):
					if letter == secretword[i]:
						print(GREEN + letter + RESET, end = "")
					elif letter in secretword:
						print(ORANGE + letter + RESET, end = "")
					else:
						print(letter, end = "")
			if attempt == wordletries - 1:
				print()
				print("You lose", end = "")
				print()
				break
		else:
			print("Word needs to be 5 letters only", end = "")
			print()
			attempt = attempt - 1


4.Zoo

class Food:
    def __init__(self, name):
        self.name = name


class Animal:
    def __init__(self, name, species):
        self.name = name
        self.species = species
        self.hungry = True

    def eat(self, food):
        raise NotImplementedError


class Carnivore(Animal):
    def eat(self, food):
        if not self.hungry:
            print(self.name, "is not hungry")
            return

        if food.name == "Meat":
            print(self.name, "eats meat")
            self.hungry = False
        else:
            print(self.name, "does not eat", food.name)


class Herbivore(Animal):
    def eat(self, food):
        if not self.hungry:
            print(self.name, "is not hungry")
            return

        if food.name in ("Grass", "Fruit"):
            print(self.name, "eats", food.name)
            self.hungry = False
        else:
            print(self.name, "does not eat", food.name)


class Cage:
    def __init__(self, number):
        self.number = number
        self.animals = []

    def add_animal(self, animal):
        self.animals.append(animal)
        print(animal.name, "added to cage", self.number)


class Worker:
    def __init__(self, name):
        self.name = name

    def feed(self, animals, food):
        print(self.name, "feeds animals with", food.name)
        for animal in animals:
            animal.eat(food)


meat = Food("Meat")
grass = Food("Grass")

lion = Carnivore("Simba", "Lion")
cow = Herbivore("Burenka", "Cow")

cage1 = Cage(1)
cage1.add_animal(lion)
cage1.add_animal(cow)

worker = Worker("Aram")
worker.feed(cage1.animals, grass)
worker.feed(cage1.animals, meat)

secretword = "bread"
wordle(secretword)		
    print(subject,'=',avg1)
